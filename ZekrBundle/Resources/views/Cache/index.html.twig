{% extends ':admin:base.html.twig' %}
{% import "AdminBundle:Main:_helper.html.twig" as helper %}

{% set sideMenuActiveSection = 'zekr' %}

{% block breadcrumb %}
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="{{ path('admin_main_home') }}">الرئيسة</a>
            </li>
            <li class="active">التخزين المؤقت</li>
        </ul>
    </div>
{% endblock %}


{% block content %}

    <div class="page-header">
        <h1>التخزين المؤقت</h1>
    </div>

    <div class="btn-group fly">
        <a class="btn btn-sm btn-success" href="{{ path('zekr_cache_remove') }}">حذف التخزين المؤقت</a>
    </div>

{% endblock %}

{% block bottomScripts %}
    {{ helper.gridJavaScripts(path('zekr_cache_index')) }}
{% endblock %}
